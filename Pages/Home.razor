@page "/"
@page "/home"

<PageTitle>Dashboard</PageTitle>

@*-- Variant Four --*@
<SectionContent SectionName="TopArea">
<MudElement HtmlTag="section" class="pageheading">
    <MudStack Row="true" Wrap="Wrap.Wrap" Justify="Justify.SpaceBetween">
        <MudStack Row="false" Spacing="1">
            <MudText Typo="Typo.h3" class="page-title">Dashboard</MudText>
            <MudText Typo="Typo.caption" class="page-subtitle">view the new enhanced dashboard</MudText>
        </MudStack>
        <MudStack Row="true" AlignItems="MudBlazor.AlignItems.Center">
            <MudButton StartIcon="@Icons.Material.Filled.Link" Variant="MudBlazor.Variant.Text" Color="Color.Surface">Ref Link</MudButton>
            <MudButton StartIcon="@Icons.Material.Filled.Output" Variant="MudBlazor.Variant.Outlined" Color="Color.Surface">Export</MudButton>
            <MudButton StartIcon="@Icons.Material.Filled.Add" Variant="MudBlazor.Variant.Filled" Color="Color.Primary">Create</MudButton>
        </MudStack>
    </MudStack>
</MudElement>
</SectionContent>

@*-- Variant Five/Seven --*@
<SectionContent SectionName="ActionButton">
    <MudStack Row="true" AlignItems="MudBlazor.AlignItems.Center" Wrap="Wrap.Wrap">
        <MudButton StartIcon="@Icons.Material.Filled.Link" Variant="MudBlazor.Variant.Text" Color="Color.Tertiary">Ref Link</MudButton>
        <MudButton StartIcon="@Icons.Material.Filled.Output" Variant="MudBlazor.Variant.Outlined" Color="Color.Secondary">Export</MudButton>
        <MudButton StartIcon="@Icons.Material.Filled.Add" Variant="MudBlazor.Variant.Filled" Color="Color.Primary">Create</MudButton>
    </MudStack>
</SectionContent>

@*-- Variant Six/Seven --*@
<SectionContent SectionName="PageHeadingTitle">
    <MudElement HtmlTag="div" Class="appbar-pageheading">
        <MudText Typo="Typo.h6" class="appbar-pagetitle">Dashboard Statistics</MudText>
        <MudBreadcrumbs Items="_items" Class="mud-typography-caption"></MudBreadcrumbs>
    </MudElement>
</SectionContent>
 

